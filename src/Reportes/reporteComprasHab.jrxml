	<parameter name="nombreC" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    ch.`id_compra` AS ID_COMPRA,
    c.`nombre_completo` AS CLIENTE,
    ch.`tipo_habitacion` AS TIPO_HABITACION,
    ch.`num_habitacion` AS NUM_HABITACION,
    ch.`precio` AS PRECIO,
    ch.`metodo_pago` AS METODO_PAGO,
    ch.`subtotal` AS SUBTOTAL,
    ch.`impuesto` AS IMPUESTO,
    ch.`total` AS TOTAL,
    ch.`pago` AS PAGO,
    ch.`cambio` AS CAMBIO,
    ch.`fecha_entrada` AS FECHA_ENTRADA,
    ch.`fecha_salida` AS FECHA_SALIDA,
    ch.`estado_salida` AS ESTADO_SALIDA
FROM
    bd_el_suenito.`compra_habitaciones` ch
INNER JOIN
    bd_el_suenito.`clientes` c
    ON ch.`id_cliente` = c.`id_cliente`
WHERE (
    $P{nombreC} IS NULL
    OR TRIM($P{nombreC}) = ''
    OR c.`nombre_completo` LIKE CONCAT('%', $P{nombreC}, '%')
);]]>
	</queryString>
